{
  "presets": [
    "next/babel",
    "@babel/preset-react"
  ],
  "babel-plugin-transform-typescript-metadata",
  [
    "@babel/plugin-proposal-decorators",
    {
      "legacy": true
    }
  ],
  "babel-plugin-parameter-decorator",
  "plugins": [
    [
      "import",
      {
        "libraryName": "vant",
        // true 的时候才会编译less
        "style": true
      }
    ]
  ]
}